#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/astro@5.14.4_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_qzdojf525lnjidejbs4xw4qwi4/node_modules/astro/node_modules:/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/astro@5.14.4_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_qzdojf525lnjidejbs4xw4qwi4/node_modules:/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/astro@5.14.4_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_qzdojf525lnjidejbs4xw4qwi4/node_modules/astro/node_modules:/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/astro@5.14.4_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_qzdojf525lnjidejbs4xw4qwi4/node_modules:/mnt/c/Users/a8999/Downloads/fuwa/locary/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
